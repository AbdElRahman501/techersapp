{
  "expo": {
    "name": "alostaz",
    "description": "Your perfect app to find your perfect teacher",
    "slug": "alostaz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/176acfb0-01b9-41df-86a5-34e4dea32ee3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.bedoahmed.alostaz"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "176acfb0-01b9-41df-86a5-34e4dea32ee3"
      }
    },
    "owner": "bedoahmed",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Al-Ostaz to use your location."
        }
      ]
    ]
  }
}